{
  "name": "flightcompanies_backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "eslint": "eslint src/**/*.ts",
    "eslint:fix": "eslint --fix src/**/*.ts",
    "prettier": "prettier src/**/*.ts",
    "prettier:fix": "prettier --write src/**/*.ts",
    "clean": "rm -rf dist/ || true",
    "build": "npx tsc",
    "start": "node ./dist/server.js",
    "migrate": "set ENV=dev && db-migrate --env dev down -c 10 && db-migrate --env dev up ",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
    "jasmine": "ENV=test && db-migrate --env test down -c 10 && db-migrate --env test up && jasmine-ts && db-migrate db:drop test && db-migrate --env test down -c 10",
    "jasmine-windows": "set ENV=test&& db-migrate --env test down -c 410 && db-migrate --env test up && jasmine-ts && db-migrate db:drop test && db-migrate --env test down -c 10",
    "test": "npm run build && npm run jasmine",
    "test-windows": "npm run build && npm run jasmine-windows",
    "deploy": "chmod +x ./bin/deploy.sh && ./bin/deploy.sh"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/validatorjs": "^3.15.0",
    "argon2": "^0.30.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "pg": "^8.8.0",
    "validatorjs": "^3.22.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jasmine": "^4.3.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/lodash": "^4.14.191",
    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "eslint": "^8.25.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine": "^4.4.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.4.0",
    "prettier": "^2.7.1",
    "prettier-airbnb-config": "^1.0.0",
    "supertest": "^6.3.0",
    "ts-node": "^10.9.1",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.8.4"
  }
}
